# Gemfile

source 'https://rubygems.org'

# This will help ensure the proper Jekyll version is running.
# Happy Jekylling!
gem 'jekyll', '~> 4.2'

# kramdown + parser
gem "kramdown", "~> 2.4"
gem "kramdown-parser-gfm", "~> 1.1.0"

# This is the default theme for new Jekyll sites.
gem "minima", "~> 2.5"

